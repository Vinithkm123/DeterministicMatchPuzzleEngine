from engine.board import Board

def seed_initial_board(level):
    board = Board()

    # Anchor pairs (guaranteed solvable)
    base_pairs = [
        [5,5,3,7,2,8,4,6,1],
        [1,9,4,6,2,8,3,7,5],
        [6,4,7,3,8,2,9,1,5]
    ]

    for row in base_pairs:
        board.add_row(row)

    return board
